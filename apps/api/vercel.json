{
  "outputDirectory": "dist",
  "builds": [
    {
      "src": "dist/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/main.js"
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "https://qa-expert.vercel.app/api"
    },
    {
      "source": "/:match*",
      "destination": "https://qa-expert.vercel.app/api/:match*"
    }
  ]
}
